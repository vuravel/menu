{
    "name": "vuravel/menu",
    "description": "Laravel and Vue.js Menus, Toolbars, Sidebars or Navigations",
    "keywords": [
        "laravel",
        "vuravel",
        "sidebar",
        "navbar",
        "menu",
        "dropdown",
        "collapse"
    ],
    "license": "MIT",
    "require": {
    	"php": ">=7.1.0",
        "laravel/framework": ">=5.6.0",
        "vuravel/elements": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Vuravel\\Menu\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vuravel\\Menu\\VuravelMenuServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
